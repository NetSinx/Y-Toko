{
  "name": "ytoko",
  "version": "1.0.0",
  "description": "project to create a E-Commerce Website it's Y-Toko",
  "main": "./src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon ./src/index.ts",
    "migrate-create": "npx typeorm-ts-node-commonjs migration:create ./src/migrations/migrations",
    "migrate-gen": "npx typeorm-ts-node-commonjs migration:generate ./src/migrations/migrations -d ./src/config/DataSource.ts",
    "migrate-run": "npx typeorm-ts-node-commonjs migration:run -d ./src/config/DataSource.ts",
    "migrate-revert": "npx typeorm-ts-node-commonjs migration:revert -d ./src/config/DataSource.ts",
    "schema-drop": "npx typeorm-ts-node-commonjs schema:drop -d ./src/config/DataSource.ts"
  },
  "keywords": [
    "e-commerce",
    "website",
    "bussiness"
  ],
  "author": "NetSinx",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jose": "^5.1.3",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
